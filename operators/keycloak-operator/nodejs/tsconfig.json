{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "index.ts",
        "keycloak/index.ts",
        "keycloak/v1alpha1/index.ts",
        "keycloak/v1alpha1/keycloak.ts",
        "keycloak/v1alpha1/keycloakBackup.ts",
        "keycloak/v1alpha1/keycloakClient.ts",
        "keycloak/v1alpha1/keycloakRealm.ts",
        "keycloak/v1alpha1/keycloakUser.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
