        "operator/index.ts",
        "operator/v1alpha1/aquaCsp.ts",
        "operator/v1alpha1/aquaDatabase.ts",
        "operator/v1alpha1/aquaEnforcer.ts",
        "operator/v1alpha1/aquaGateway.ts",
        "operator/v1alpha1/aquaScanner.ts",
        "operator/v1alpha1/aquaServer.ts",
        "operator/v1alpha1/index.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
